auth_token = os.environ['TWILIO_AUTH_TOKEN']
client = Client(account_sid, auth_token)

address = client.addresses.create(
                               friendly_name='Twilio',
                               emergency_enabled=True,
                               customer_name='Twilio',
                               street='645 Harrison St.',
                               city='San Francisco',
                               region='CA',
                               postal_code='94105',
                               iso_country='US'
                           )

print(address.sid)
